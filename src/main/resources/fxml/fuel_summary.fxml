<VBox alignment="CENTER" prefHeight="270.0" prefWidth="367.0" spacing="10" style="-fx-background-color: slategray;" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/24.0.1" fx:controller="fuelapp.controllers.FuelSummaryController">
    <Label fx:id="summaryLabel" text="Fuel Quote Summary">
      <font>
         <Font name="System Bold" size="12.0" />
      </font></Label>
    <Button onAction="#goToLogin" text="Back to Login">
      <font>
         <Font name="System Bold Italic" size="12.0" />
      </font></Button>
</VBox>
